<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="CDP Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="dialog-width-50rem">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <cdp
                jcr:primaryType="nt:unstructured"
                jcr:title="CDP"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container"
                        class="column-full-width">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please Enter The Partner Page CDP Code Heading"
                                fieldLabel="Heading"
                                name="./heading"
                                required="{Boolean}true"/>
                            <subheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please Enter The Partner Page CDP Code Subheading"
                                fieldLabel="Subheading"
                                name="./subheading"/>
                            <buttontext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please Enter The Partner Page CDP Code Button Text"
                                fieldLabel="Button Text"
                                name="./buttontext"
                                required="{Boolean}true"/>
                            <cdpcodeprefix
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                cdpField=""
                                fieldDescription="Please Enter The Partner Page CDP Code Prefix"
                                fieldLabel="CDP Label"
                                name="./cdpcodeprefix"/>
                            <cdpcode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                cdpField=""
                                fieldDescription="Please Enter The Partner Page CDP Code"
                                fieldLabel="CDP Code"
                                name="./cdpcode"/>
                            <pccodeprefix
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please Enter The Partner Page CDP Code Prefix"
                                fieldLabel="PC Label"
                                name="./pccodeprefix"
                                pcfield=""/>
                            <pccode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please Enter The Partner Page CDP Code"
                                fieldLabel="PC Code"
                                name="./pccode"
                                pcfield=""/>
                        </items>
                    </column>
                </items>
            </cdp>
        </items>
    </content>
</jcr:root>
