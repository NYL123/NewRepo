<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Country-Language"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">
            <countrylanguageconfiguration
                jcr:primaryType="nt:unstructured"
                jcr:title="Country-Language"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                    	<countryname
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Country Name."
                        fieldLabel="Country Name"
                        name="./countryName"
                        required="{Boolean}true"/>
                        <countrycode
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Country Code."
                        fieldLabel="Country Code"
                        name="./countryCode"
                        required="{Boolean}true"/>
                        <languagename
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Language Name."
                        fieldLabel="Language Name"
                        name="./languageName"
                        required="{Boolean}true"/>
                        <languagecode
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Language Code."
                        fieldLabel="Language Code"
                        name="./languageCode"
                        required="{Boolean}true"/>
                        <languageprefix
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Language Prefix."
                        fieldLabel="Language Prefix"
                        name="./languagePrefix"/>
                        <countryhostname
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Hostname."
                        fieldLabel="Hostname"
                        name="./countryHostname"
                        required="{Boolean}true"/>
                		</items>
                    </column>
                </items>
            </countrylanguageconfiguration>
        </items>
    </content>
</jcr:root>
