<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Country Language"
    sling:resourceType="cq/gui/components/authoring/dialog">
 <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="dialog-width-50rem">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">
        	<countrylanguage
                cq:showOnCreate="{Boolean}false"
                jcr:primaryType="nt:unstructured"
                jcr:title="Country Language"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
           		<column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    	<countrylabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Country Dropdown Label."
                        fieldLabel="Country Dropdown Label"
                        name="./countryLabel"/>
                        <languagelabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Language Dropdown Label"
                        fieldLabel="Language Dropdown Label"
                        name="./languageLabel"/>
                        <buttonlabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Button label."
                        fieldLabel="Update CTA Label"
                        name="./buttonLabel"/>
                       <updateMessage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the Update Message."
                        fieldLabel="Update Message"
                        name="./updateMessage"/>
                        <noresultsMessage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter the No Results Message."
                        fieldLabel="No Results Message"
                        name="./noresultsMessage"/>
                 </items>
            </column>
        </items>
       </countrylanguage>
       </items>
    </content>
</jcr:root>