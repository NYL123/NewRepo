                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Text field for Links Element Group" fieldLabel="Element Group"
                                                    name="./lelementGroup"/>
                                                <element
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Text field for Links Element " fieldLabel="Element "
                                                    name="./lelement"/>
                                                    <linkText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Text field for Link Text" fieldLabel="Link Text "
                                                    name="./llinkText"/>
                                                     <ariaLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Text field for Aria Label" fieldLabel="Aria Label"
                                                    name="./lariaLabel"/>
                                                    <targetType
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Text field for Target Type" fieldLabel="Target Type"
                                                    name="./ltargetType"/>
                                                    <targetURL
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Text field for Target URL" fieldLabel="Target URL"
                                                    name="./ltargetURL"/> 
                                            </links>  -->
								
                        </items>
                    </column>
                </items>
            </checkbox>
        </items>
    </content>
</jcr:root>
