<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Template for City Location page"
    jcr:primaryType="cq:Template"
    jcr:title="City Location Page"
    allowedPaths="[/content(/.*)?]"
	allowedParents="[/apps/hertz/templates/locationdirectory,/apps/hertz/templates/citylocation]"
    ranking="{Long}43">
    <jcr:content
        cq:deviceGroups="/etc/mobile/groups/responsive"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="hertz/components/pages/citylocation">
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Decimal}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Decimal}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
